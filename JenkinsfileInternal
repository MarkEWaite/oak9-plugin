#!/usr/bin/env groovy

node {
    try {
        stage('Build') {
            sh 'echo "Checking out code from SCM"'
            checkout scm
            sh 'echo "Building Plugin"'
            sh 'mvn install'
        }
    } finally {
              archiveArtifacts artifacts: 'target/oak9.hpi', fingerprint: true
    }
}
